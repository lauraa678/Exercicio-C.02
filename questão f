using System;

namespace Exercicíos
{
	class Program
	{
		public static void Main()
		{
			double raiz = 0;
			Console.WriteLine("Digite um número qualquer: ");
			double num1 = double.Parse(Console.ReadLine());
			
			raiz = Math.Sqrt(num1);
			
			Console.WriteLine("A raiz quadrada desse número é: " + raiz );
			Console.WriteLine("");
			Console.Write("Pressione uma tecla qualquer . . . ");
			Console.ReadKey(true);
		}
	}
}
